@model Mvc3QuizQuestions.Models.AnswerQuiz
@{
    ViewBag.Title = "Delete";
}
<div class="page-header">
    <h2>@ViewBag.Title</h2>
</div>
<h3>
    Are you sure you want to delete this?</h3>
@using (Html.BeginForm())
{
    <div>
        <fieldset>
            <legend>AnswerQuiz</legend>
            <br />
            <div class="row">
                <div class="span2">@Html.LabelFor(model => model.User)</div>
                <div class="span10">@Html.DisplayFor(model => model.User)</div>
            </div>
            <div class="row">
                <div class="span2">@Html.LabelFor(model => model.AnsweredOn)</div>
                <div class="span10">@Html.DisplayFor(model => model.AnsweredOn)</div>
            </div>
            <div class="row">
                <div class="span2">@Html.LabelFor(model => model.Score)</div>
                <div class="span10">@Html.DisplayFor(model => model.Score)</div>
            </div>
            <div class="row">
                <div class="span2">@Html.LabelFor(model => model.CreatedOn)</div>
                <div class="span10">@Html.DisplayFor(model => model.CreatedOn)</div>
            </div>
            <div class="row">
                <div class="span2">@Html.LabelFor(model => model.UpdatedOn)</div>
                <div class="span10">@Html.DisplayFor(model => model.UpdatedOn)</div>
            </div>
            <div class="form-actions">
                <input type="submit" class="btn btn-danger" value="Delete" />
                @Html.ActionLink("Cancel", "Index", null, new { @class = "btn" })
            </div>
        </fieldset>
    </div>
}
